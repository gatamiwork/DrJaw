<Window x:Class="DrJaw.Views.User.TransferOut"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DrJaw.Views.User"
        mc:Ignorable="d"
        Title="TransferOut" Height="92" Width="243">
    <Grid>
        <ComboBox x:Name="comboboxTransfer" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="207" DisplayMemberPath="Name" SelectedValuePath="Id"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top" Width="207" Click="Action"/>
    </Grid>
</Window>
